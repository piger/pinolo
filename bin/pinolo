#!/usr/bin/env python

# PATH FIX
import sys
import os
parent = os.path.realpath(os.path.dirname(__file__))
parent = os.path.split(parent)[0]
sys.path.insert(0, parent)
# END

import sys
import gettext
from pinolo import args, commands

gettext.install('pinolo')
args.parse_and_run_command(sys.argv[1:], commands, default_command='help')
